<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0" prefWidth="600.0"
            xmlns="http://javafx.com/javafx/8.0.111" fx:controller="app.BrokerController">
    <children>
        <Pane layoutX="27.0" layoutY="42.0" prefHeight="317.0" prefWidth="292.0">
            <children>
                <TableView fx:id="tvTracks" layoutX="-2.0" prefHeight="317.0" prefWidth="301.0">
                    <columns>
                        <TableColumn fx:id="tcName" prefWidth="98.0" text="Name"/>
                        <TableColumn fx:id="tcArtists" prefWidth="104.0" text="Artists"/>
                        <TableColumn fx:id="tcDuration" prefWidth="98.0" text="Duration"/>
                    </columns>
                </TableView>
            </children>
        </Pane>
        <Button layoutX="350.0" layoutY="328.0" mnemonicParsing="false" onAction="#btnSendClick" text="Send"/>
        <Button layoutX="418.0" layoutY="328.0" mnemonicParsing="false" onAction="#btnPlayTracksClick"
                text="Play Tracks"/>
        <TextField fx:id="tfGroup" layoutX="349.0" layoutY="42.0" prefHeight="31.0" prefWidth="162.0"/>
        <Button layoutX="349.0" layoutY="84.0" mnemonicParsing="false" onAction="#btnSetGroupClick" prefHeight="31.0"
                prefWidth="162.0" text="Set Group"/>
    </children>
</AnchorPane>
